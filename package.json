    "@storybook/addon-ondevice-controls": "^10.0.2",
    "@storybook/react-native": "^10.0.2",
    "@storybook/react-native-web-vite": "^10.0.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/i18n-js": "^3.8.9",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.1.0",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.13",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
