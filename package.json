{
  "name": "taskmanager",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "storybook:start": "storybook generate && storybook dev -p 7007",
    "storybook:web": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:preview": "storybook preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:snapshot": "jest --testPathPattern=snapshot",
    "test:snapshot:update": "jest --testPathPattern=snapshot --updateSnapshot",
    "test:update-snapshots": "jest --updateSnapshot",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src/**/*.tsx src/**/*.ts",
    "build": "expo build"
  },
  "lint-staged": {
   "src/**/*.{ts,tsx,js,jsx}": [
    "eslint --fix",
    "prettier --write"
  ]
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
